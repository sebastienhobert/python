def encrypt(key, plaintext):
    alphabet = "abcdefghijklmnopqrstuvwxyz"
    encrypted_text = ""

    for char in plaintext:
        if char.isalpha():
            shifted_index = (alphabet.index(char) + key) % 26
            encrypted_text += alphabet[shifted_index]
